<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Lamond Lu</Authors>
    <Company>Lamond Lu</Company>
    <Description>a library to expose file in AWS S3 to ASP.NET Core Application with IFileProvider</Description>
    <PackageProjectUrl>https://github.com/lamondlu/AWSS3FileProvider</PackageProjectUrl>
    <RepositoryUrl>https://github.com/lamondlu/AWSS3FileProvider</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.3.31.19" />
    <PackageReference Include="Microsoft.AspNetCore.App" Version="2.2.2" />
  </ItemGroup>

</Project>
